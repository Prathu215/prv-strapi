{
  "kind": "collectionType",
  "collectionName": "contacts",
  "info": {
    "singularName": "contact",
    "pluralName": "contacts",
    "displayName": "contact",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "phoneNumber": {
      "type": "string"
    },
    "email": {
      "type": "email"
    },
    "message": {
      "type": "text"
    },
    "subject": {
      "type": "string"
    },
    "contacts": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "plugin::users-permissions.user"
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "Events",
        "Blogs",
        "Movie",
        "Other"
      ],
      "default": "Events"
    },
    "isReaded": {
      "type": "boolean",
      "default": false
    }
  }
}
